{
  "name": "pinceau",
  "type": "module",
  "version": "0.18.8",
  "packageManager": "pnpm@7.20.0",
  "workspaces": [
    "docs",
    "playground"
  ],
  "description": "Make your <script> lighter and your <style> smarter.",
  "license": "MIT",
  "homepage": "https://github.com/Tahul/pinceau#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tahul/pinceau.git"
  },
  "bugs": {
    "url": "https://github.com/Tahul/pinceau/issues"
  },
  "keywords": [
    "css-in-js",
    "vue",
    "design-tokens",
    "css-variables",
    "typed-styles",
    "nuxt",
    "vite"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./types": {
      "require": "./dist/index.d.ts",
      "import": "./dist/index.d.ts"
    },
    "./vite": {
      "require": "./dist/vite.cjs",
      "import": "./dist/vite.mjs"
    },
    "./nuxt": {
      "require": "./dist/nuxt.cjs",
      "import": "./dist/nuxt.mjs"
    },
    "./runtime": {
      "require": "./dist/runtime.cjs",
      "import": "./dist/runtime.mjs"
    },
    "./nitro": {
      "require": "./dist/nitro.cjs",
      "import": "./dist/nitro.mjs"
    },
    "./utils": {
      "require": "./dist/utils.cjs",
      "import": "./dist/utils.mjs"
    },
    "./volar": "./dist/volar.cjs",
    "./browser": "./dist/browser/index.js",
    "./browser/volar": "./dist/browser/volar.js",
    "./browser/runtime": "./dist/browser/runtime.js",
    "./browser/utils": "./dist/browser/utils.js"
  },
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "dist",
    "volar"
  ],
  "scripts": {
    "build": "jiti build/base.ts && jiti build/browser.ts",
    "build:browser": "jiti build/browser.ts",
    "build:base": "jiti build/base.ts",
    "dev": "pnpm -c playground run prepare && pnpm -C playground run dev",
    "lint": "eslint . --cache",
    "lint:fix": "pnpm run lint --fix",
    "build:play": "pnpm -C playground run build",
    "build:play:nuxt": "pnpm -C playground run build:nuxt",
    "play": "rm -rf playground/node_modules/.vite && pnpm -C playground run dev",
    "play:nuxt": "rm -rf playground/.nuxt && pnpm -C playground run dev:nuxt",
    "prepare:nuxt": "rm -rf playground/.nuxt && pnpm -C playground run prepare",
    "docs": "pnpm -C docs run dev",
    "build:docs": "pnpm -C docs run build",
    "prepublishOnly": "pnpm run build",
    "release": "release-it",
    "start": "esno src/index.ts",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:build": "pnpm run prepare && pnpm run build:play && pnpm run build:play:nuxt",
    "test:nuxt": "(cd test/fixtures/nuxt && npx nuxi dev)",
    "test:vite": "(cd test/fixtures/vite && npx vite dev)",
    "prepare": "pnpm -C playground run prepare && npm -C docs run prepare"
  },
  "dependencies": {
    "@unocss/reset": "^0.50.6",
    "@volar/vue-language-core": "^1.3.11",
    "acorn": "^8.8.2",
    "chroma-js": "^2.4.2",
    "consola": "^2.15.3",
    "csstype": "^3.1.2",
    "defu": "^6.1.2",
    "magic-string": "^0.30.0",
    "nanoid": "^4.0.2",
    "ohash": "^1.0.0",
    "paneer": "^0.1.0",
    "pathe": "^1.1.0",
    "postcss-custom-properties": "13.1.4",
    "postcss-dark-theme-class": "0.7.3",
    "postcss-nested": "^6.0.1",
    "recast": "^0.22.0",
    "scule": "^1.0.0",
    "sfc-composer": "^0.0.4",
    "style-dictionary-esm": "^1.3.7",
    "unbuild": "^1.2.0",
    "unplugin": "^1.3.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.38.3",
    "@nuxt/kit": "^3.3.3",
    "@nuxt/test-utils": "^3.3.3",
    "@nuxthq/studio": "^0.10.0",
    "@nuxtjs/color-mode": "^3.2.0",
    "@types/chroma-js": "^2.4.0",
    "@types/node": "^18.15.11",
    "@vue/test-utils": "^2.3.2",
    "buffer": "^6.0.3",
    "eslint": "^8.37.0",
    "esno": "^0.16.3",
    "fast-glob": "^3.2.12",
    "js-confetti": "^0.11.0",
    "jsdom": "^21.1.1",
    "nitropack": "2.3.2",
    "nodemon": "^2.0.22",
    "nuxi": "3.3.3",
    "nuxt": "3.3.3",
    "nuxt-component-meta": "^0.5.1",
    "nuxt-vitest": "^0.6.9",
    "playwright": "^1.32.2",
    "release-it": "^15.10.1",
    "rimraf": "^4.4.1",
    "rollup": "^3.20.2",
    "tsup": "^6.7.0",
    "typescript": "^5.0.3",
    "unenv": "^1.2.2",
    "vite": "^4.2.1",
    "vite-plugin-inspect": "^0.7.18",
    "vitest": "^0.29.8",
    "vue": "^3.2.47",
    "webpack": "^5.77.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "axios": "0.25.0",
        "vue": "^3.2.45",
        "vite": "^4.0.0"
      },
      "ignoreMissing": [
        "postcss",
        "postcss-*",
        "@algolia/client-search",
        "nuxt",
        "vue"
      ]
    },
    "overrides": {
      "pinceau": "latest"
    }
  },
  "resolutions": {
    "esquery": "1.4.0"
  },
  "release-it": {
    "hooks": {
      "before:init": [
        "pnpm build"
      ]
    },
    "npm": {
      "access": "public"
    },
    "git": {
      "commitMessage": "chore(release): release v${version}"
    },
    "github": {
      "release": true,
      "releaseName": "v${version}"
    }
  }
}
